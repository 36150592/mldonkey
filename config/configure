        GTOOLBOX="`$OCAMLC -where`/lablgtk/gToolbox.mli"
        grep popup $GTOOLBOX | grep -i button 2> /dev/null && GTOOLBOX_ARGS=new
        if test "$GTOOLBOX_ARGS" = "new"; then
