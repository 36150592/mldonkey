echo "$as_me:$LINENO: checking compile GTK GUI" >&5
echo $ECHO_N "checking compile GTK GUI... $ECHO_C" >&6
  if test "$LABLGTK_CONFIG" = "no"; then
        echo "$as_me:$LINENO: result: no" >&5
